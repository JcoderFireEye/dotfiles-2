XTerm*vt100.boldFont:         ShureTechMono Nerd Font:bold:size=12:antialias=true
XTerm*vt100.allowTitleOps:    true
XTerm*border:                 off
xTerm*borderWidth:            0
XTerm*Thickness:              0
xTerm*loginShell:             true
XTerm*multiScroll:            on
XTerm*background:             #002B36
